{
  "expo": {
    "name": "self-selfdriving",
    "slug": "self-selfdriving",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "selfselfdriving",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    
    "android": {
      "package": "com.yourname.selfselfdriving",  
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA6Q056BELUQAqOLuoNQwmnx89PsWf0m8Q" 
        }
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.selfselfdriving",
      "config": {
        "googleMapsApiKey": "AIzaSyA6Q056BELUQAqOLuoNQwmnx89PsWf0m8Q" 
      }
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      
      [
        "@react-native-google-signin/google-signin",
        {
          "androidClientId": "1234567890-abcxyz.apps.googleusercontent.com",
          "iosUrlScheme": "com.googleusercontent.apps.1234567890-abcxyz" 
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
